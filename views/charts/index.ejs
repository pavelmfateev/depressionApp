<% layout('layouts/boilerplate')%>
<canvas id="myChart" width="400" height="175"></canvas>

<script>
    var chartXy = <%- JSON.stringify(chartData) %>;
    console.log(chartXy.data);
    new Chart(document.getElementById("myChart"), {
    type: "line",
    data: {
      datasets: [
        {
          data: chartXy.data,
          fill: false,
          borderColor: 'rgba(76, 178, 178, 0.9)'
        },
      ],
    },
    options: {
        legend: {
            display: false
        },
      scales: {
        xAxes: [
          {
            type: "time",
            time: {
              unit: "week",
            },
            distribution: "linear",
          },
        ],
      },
    }
  });
</script>
